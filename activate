#PRINT_DIRECTORY_STRUCTUR
pds() {
    ./scripts/cpp/dst/print_directory_structure	
}

#VSCODE_CONFIG_MANAGER
vcm() {
    case "$1" in
        cpp)
            ./scripts/cpp/dst/vscode_config_manager create ./configs/cpp/.vscode
            ;;
        py)
            ./scripts/cpp/dst/vscode_config_manager create ./configs/py/.vscode
            ;;
        *)
            echo -e "Invalid argument: "$1".\nUse: 'cpp' or 'py'" >&2
            return 1
            ;;
    esac
}
